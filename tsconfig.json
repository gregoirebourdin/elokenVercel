{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    // Using "baseUrl" allows you to use absolute paths
    "baseUrl": ".",
    // Using "paths" allows you to configure module path aliases
    "paths": {
      "@/templates/*": [
        "src/templates/*"
      ],
      "@/logos/*": [
        "src/assets/medias/logos/*"
      ],
      "@/images/*": [
        "src/assets/medias/images/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/library/*": [
        "src/library/*"
      ],
      "@/context/*": [
        "src/context/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/base/*": [
        "src/base/*"
      ],
      "@/medias/*": [
        "src/assets/medias/*"
      ],
      "@/styles/*": [
        "src/assets/styles/*"
      ],
      "@/routes/*": [
        "src/routes/*"
      ],
    },
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
,
    "src/templates/Wireframes/Title"
  ],
  "exclude": [
    "node_modules"
  ],
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}
